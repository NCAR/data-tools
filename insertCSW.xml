<csw:Transaction service="CSW" version="2.0.2"
                 xmlns:csw="http://www.opengis.net/cat/csw/2.0.2">
    <csw:Insert>
<MD_Metadata xmlns="http://www.isotc211.org/2005/gmd" xmlns:gco="http://www.isotc211.org/2005/gco" xmlns:gml="http://www.opengis.net/gml" xmlns:xlink="http://www.w3.org/1999/xlink">
 <!-- ***** DATACITE: doi ***** -->
    <fileIdentifier>
        <gco:CharacterString>FILE_IDENTIFIER</gco:CharacterString>
    </fileIdentifier>
    <language>
        <gco:CharacterString>en</gco:CharacterString>
    </language>
 <!-- ***** DATACITE: resourceTypeGeneral ***** -->
 <!-- NOTE: allowed values for hierarchyLevel are:  attribute, attributeType, collectionHardware, collectionSession, dataset, series, nonGeographicDataset, dimensionGroup, feature, featureType, propertyType, fieldSession, software, service, model, tile.  Default value is "dataset". -->
    <hierarchyLevel>
        <MD_ScopeCode codeList="http://www.isotc211.org/2005/resources/codeList.xml#MD_ScopeCode" codeListValue="dataset"/>
    </hierarchyLevel>
    <hierarchyLevelName>
        <gco:CharacterString>RESOURCE_TYPE_GENERAL</gco:CharacterString>
    </hierarchyLevelName>
 <!-- ***** DATACITE: date  (dateType == dateAvailable?)  ***** -->
    <!-- Date the metadata was created -->
    <dateStamp>
        <gco:Date>2006-12-15</gco:Date>
    </dateStamp>
    <metadataStandardName>
        <gco:CharacterString>ISO 19115 Geographic Information Metadata</gco:CharacterString>
    </metadataStandardName>
    <metadataStandardVersion>
        <gco:CharacterString>2003/Cor.1:2006</gco:CharacterString>
    </metadataStandardVersion>
    <identificationInfo>
        <MD_DataIdentification>
            <citation>
                <CI_Citation>
 <!-- ***** DATACITE: title ***** -->
                    <title>
                        <gco:CharacterString>TITLE</gco:CharacterString>
                    </title>
 <!-- ***** DATACITE: publicationYear ***** -->
                    <date>
                        <CI_Date>
                            <date>
                                <gco:Date>PUBLICATION_YEAR</gco:Date>
                            </date>
                            <dateType>
                                <CI_DateTypeCode codeList="http://www.isotc211.org/2005/resources/codeList.xml#CI_DateTypeCode" codeListValue="publication"/>
                            </dateType>
                        </CI_Date>
                    </date>
                </CI_Citation>
            </citation>
 <!-- ***** DATACITE: creator, publisher, contributor ***** -->
           <pointOfContact>
               <CI_ResponsibleParty>
                   <individualName>
                       <gco:CharacterString>MISSING_AUTHOR</gco:CharacterString>
                   </individualName>
                   <role>
                       <CI_RoleCode codeList="http://www.isotc211.org/2005/resources/codeList.xml#CI_RoleCode" codeListValue="MISSING_ROLE"/>
                   </role>
                   <organisationName gco:nilReason="missing">
                        <gco:CharacterString/>
                   </organisationName>
                   <positionName gco:nilReason="missing">
                      <gco:CharacterString/>
                   </positionName>
                   <contactInfo>
                      <CI_Contact>
                         <phone>
                            <CI_Telephone>
                               <voice gco:nilReason="missing">
                                  <gco:CharacterString/>
                               </voice>
                               <facsimile gco:nilReason="missing">
                                  <gco:CharacterString/>
                               </facsimile>
                            </CI_Telephone>
                         </phone>
                         <address>
                            <CI_Address/>
                         </address>
                      </CI_Contact>
                   </contactInfo>
               </CI_ResponsibleParty>
           </pointOfContact>
 <!-- ***** DATACITE: description ***** -->
            <abstract>
                <gco:CharacterString>
                    DESCRIPTION
                </gco:CharacterString>
            </abstract>
 <!-- ***** DATACITE: Subject ***** -->
            <descriptiveKeywords>
                <MD_Keywords>
                    <keyword>
                        <gco:CharacterString>KEYWORD</gco:CharacterString>
                    </keyword>
                    <type>
                        <MD_KeywordTypeCode codeList="http://metadata.dgiwg.org/codelistRegistry?MD_KeywordTypeCode" codeListValue="theme"/>
                    </type>
                </MD_Keywords>
            </descriptiveKeywords>
            <language>
                <gco:CharacterString>en</gco:CharacterString>
            </language>
            <topicCategory>
                <MD_TopicCategoryCode>geoscientificInformation</MD_TopicCategoryCode>
            </topicCategory>
        </MD_DataIdentification>
    </identificationInfo>
<!-- ***** DATACITE: relatedIdentifier ***** -->
    <distributionInfo>
      <MD_Distribution>
        <transferOptions>
          <MD_DigitalTransferOptions>
            <onLine>
              <CI_OnlineResource>
                <linkage>
                  <URL>HTTP_REFERENCE</URL>
                </linkage>
                <protocol>
                  <gco:CharacterString>WWW:LINK-1.0-http--link</gco:CharacterString>
                </protocol>
                <name>
                  <gco:CharacterString>BROWSER_NAME</gco:CharacterString>
                </name>
                <description gco:nilReason="missing">
                  <gco:CharacterString />
                </description>
              </CI_OnlineResource>
            </onLine>
          </MD_DigitalTransferOptions>
        </transferOptions>
      </MD_Distribution>
    </distributionInfo>
</MD_Metadata>
    </csw:Insert>
</csw:Transaction>
